"Customize my own groups
if !exists('g:spf13_bundle_groups')
    let g:spf13_bundle_groups=['general', 'youcompleteme', 'programming', 'python', 'go', 'javascript', 'html', 'misc']
endif

"Disable automatic view restoring
let g:spf13_no_views = 1

"Disable filetype before loading vundle packages
filetype off

"spf13-vim is breaking the arrow key in insert mode, disable the omni function
"for now
let g:spf13_no_omni_complete = 1
